<div class="newForm">
  <%= form_with url: session_path do |form| %>
    <div class="mb-3 form-group">
      <%= form.label :email, class: "form-label fs-5"%>
      <%= form.text_field :email, class: "form-control"%>
    </div>
    <div>
      <%= form.label :password, class: "form-label fs-5"%>
      <%= form.text_field :password, class: "form-control"%>
    </div>
  <br>
    <%= form.submit "Sign In", class: "btn btn-outline-primary" %>
  <% end %>
</div>